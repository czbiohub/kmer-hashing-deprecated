<script type="text/javascript">
var handler=null;
// see https://github.com/mbostock/d3/wiki/Ordinal-Scales#category20c
var colors = d3.scale.category20c().domain(d3.range(0,20)).range()

function c0(index) { return "#9c9c9c"; }
function c1(index) { return "#bdbdbd"; }
function c2(index) { return colors[index % 16]; } // <-- note: uses only the first 16 colors

var elapsed="5.7s"
var beginningMillis=1551488321773;
var endingMillis=1551488327465;
var data=[
{"label": "filter (SRR5810748_filter)", "times": [{"starting_time": 1551488324013, "ending_time": 1551488324181, "color":c1(0)}, {"starting_time": 1551488324181, "ending_time": 1551488324629, "color":c2(0), "label": "616ms \/ -"}]},
{"label": "filter (SRR5810768_filter)", "times": [{"starting_time": 1551488324649, "ending_time": 1551488324729, "color":c1(0)}, {"starting_time": 1551488324729, "ending_time": 1551488324871, "color":c2(0), "label": "222ms \/ -"}]},
{"label": "filter (SRR5810765_filter)", "times": [{"starting_time": 1551488324881, "ending_time": 1551488324971, "color":c1(0)}, {"starting_time": 1551488324971, "ending_time": 1551488325105, "color":c2(0), "label": "224ms \/ -"}]},
{"label": "filter (SRR5810758_filter)", "times": [{"starting_time": 1551488325113, "ending_time": 1551488325205, "color":c1(0)}, {"starting_time": 1551488325205, "ending_time": 1551488325349, "color":c2(0), "label": "236ms \/ -"}]},
{"label": "filter (SRR5810750_filter)", "times": [{"starting_time": 1551488325356, "ending_time": 1551488325449, "color":c1(0)}, {"starting_time": 1551488325449, "ending_time": 1551488325583, "color":c2(0), "label": "227ms \/ -"}]},
{"label": "filter (SRR5810771_filter)", "times": [{"starting_time": 1551488325590, "ending_time": 1551488325683, "color":c1(0)}, {"starting_time": 1551488325683, "ending_time": 1551488325810, "color":c2(0), "label": "220ms \/ -"}]},
{"label": "filter (SRR5810720_filter)", "times": [{"starting_time": 1551488325815, "ending_time": 1551488325910, "color":c1(0)}, {"starting_time": 1551488325910, "ending_time": 1551488326039, "color":c2(0), "label": "224ms \/ -"}]},
{"label": "filter (SRR5810729_filter)", "times": [{"starting_time": 1551488326048, "ending_time": 1551488326139, "color":c1(0)}, {"starting_time": 1551488326139, "ending_time": 1551488326291, "color":c2(0), "label": "243ms \/ -"}]},
{"label": "filter (SRR5810685_filter)", "times": [{"starting_time": 1551488326299, "ending_time": 1551488326391, "color":c1(0)}, {"starting_time": 1551488326391, "ending_time": 1551488326531, "color":c2(0), "label": "232ms \/ -"}]},
{"label": "filter (SRR5810763_filter)", "times": [{"starting_time": 1551488326538, "ending_time": 1551488326632, "color":c1(0)}]}
]


function getTickFormat() {
  var MIN = 1000 * 60
  var HOUR = MIN * 60
  var DAY = HOUR * 24
  var delta = (endingMillis - beginningMillis)

  if( delta < 2 * MIN ) {
    return {
      format: d3.time.format("%S"),
      tickTime: d3.time.seconds,
      tickInterval: 5,
      tickSize: 6
    }
  }

  if( delta < 2 * HOUR ) {
    return {
      format: d3.time.format("%M"),
      tickTime: d3.time.minutes,
      tickInterval: 5,
      tickSize: 6
    }
  }

  if( delta < 2 * DAY ) {
    return {
      format: d3.time.format("%H:%M"),
      tickTime: d3.time.hours,
      tickInterval: 1,
      tickSize: 6
    }
  }

  if( delta <= 7 * DAY ) {
    return {
      format: d3.time.format("%b %e %H:%M"),
      tickTime: d3.time.hours,
      tickInterval: 6,
      tickSize: 6
    }
  }

  return {
    format: d3.time.format("%b %e"),
    tickTime: d3.time.days,
    tickInterval: 1,
    tickSize: 6
  }
}

function getLabelMargin(scale) {
    $('<span class="labelSpan" style="display: none"></span>').appendTo('body');

    var labelMargin = 0
    $.each(data, function (key, value) {
      labelMargin = Math.max(labelMargin, $('.labelSpan').html(value.label).width());
    });

    $('.labelSpan').remove();

    return (labelMargin * scale);
}

function render() {
  handler=null;
  $("#timeline").empty()
  $('#label_elapsed').text(elapsed)
  $('#label_launch').text( d3.time.format('%d %b %Y %H:%M')(new Date(beginningMillis)) )

  var width = $(window).width();
  var chart = d3.timeline()
    .stack() // toggles graph stacking
    .margin({left:getLabelMargin(0.85), right:100, top:0, bottom:0})
    .tickFormat( getTickFormat() )
    .rowSeperators('#f5f5f5')
    .showTimeAxisTick()
    ;
  var svg = d3.select("#timeline").append("svg").attr("width", width).datum(data).call(chart);
}

function hrz() {
if( handler != null ) clearTimeout(handler)
  handler = setTimeout(render, 150);
}

$(document).ready(render)
$(window).resize(hrz); // resize the applet on window resize
</script>
</head>

